	<RelativeLayout android:id="@+id/sent_appointment_screen"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/our_story_color">

		<RelativeLayout
			android:id="@+id/content_container"
			android:layout_width="match_parent"
			android:layout_height="match_parent">


			<RelativeLayout
				android:id="@+id/page_sent_appointment_screen_ek1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp">

				<!--				<View-->
				<!--					android:id="@+id/_bg__sent_appointment_screen_ek2"-->
				<!--					android:layout_width="match_parent"-->
				<!--					android:layout_height="match_parent"-->
				<!--					android:layout_marginStart="0dp"-->
				<!--					android:layout_marginTop="0dp"-->
				<!--					android:background="@drawable/_bg__sent_appointment_screen_ek2_shape" />-->


				<RelativeLayout
					android:id="@+id/bookappointment_title_frame"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_marginTop="40dp">

					<TextView
						android:id="@+id/book_your_appointment_"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="Inter"
						android:text="@string/book_your_appointment__string"
						android:textColor="@color/_bg__homescreen_ek2_color"
						android:textSize="32sp"
						android:textStyle="bold" />

				</RelativeLayout>


				<RelativeLayout
					android:id="@+id/appointment_form_frame"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/book_your_appointment_"
					android:layout_marginStart="0dp"
					android:layout_marginTop="150dp"
					android:background="@drawable/bookappointmtnt_backgroundshape"
					tools:ignore="NotSibling">


					<LinearLayout
						android:id="@+id/appointmentlayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_centerVertical="true"
						android:layout_marginStart="30dp"
						android:layout_marginEnd="30dp"
						android:gravity="left"
						android:orientation="vertical">


						<EditText
							android:id="@+id/textView3"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:hint="Full Name"
							android:inputType="text" />

						<EditText
							android:id="@+id/editTextPhone"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:ems="10"
							android:hint="Contact Number"
							android:inputType="phone" />

						<EditText
							android:id="@+id/editTextTextEmailAddress"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:ems="10"
							android:hint="Email Address ( Optionl)"
							android:inputType="textEmailAddress" />

						<EditText
							android:id="@+id/editTextDate"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:ems="10"
							android:hint="Date"
							android:inputType="date" />

						<EditText
							android:id="@+id/editTextTextPersonName2"
							android:layout_width="match_parent"
							android:layout_height="100dp"
							android:ems="10"
							android:hint="Eneter Your Message"
							android:inputType="textPersonName" />


					</LinearLayout>

					<Button
						android:layout_width="wrap_content"
						android:layout_height="50dp"
						android:layout_below="@+id/appointmentlayout"
						android:layout_alignParentEnd="true"
						android:layout_marginTop="24dp"
						android:layout_marginEnd="29dp"
						android:background="@drawable/hmscreentxtview_bg_gradiant"
						android:gravity="center"
						android:text="SUBMIT" />
				</RelativeLayout>

			</RelativeLayout>
		</RelativeLayout>

	</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>